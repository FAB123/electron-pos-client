{
  "name": "salestime-pos-react",
  "productName": "SalesTime POS",
  "version": "1.1.2",
  "description": "Point of Sale",
  "author": "AL Hasib Jeddha",
  "build": {
    "appId": "<com.ahcjed>",
    "win": {
      "icon": "build/icons.png"
    },
    "publish": {
